def encode(st):
    values={'a':'1','e':'2','i':'3','o':'4','u':'5'}
    result=''
    for i in st:
        if i in values.keys():
            result+=values[i]
        else:
            result+=i
    return result
    
def decode(st):
    values={'1':'a','2':'e','3':'i','4':'o','5':'u'}
    result=''
    for i in st:
        if i in values.keys():
            result+=values[i]
        else:
            result+=i
    return result
